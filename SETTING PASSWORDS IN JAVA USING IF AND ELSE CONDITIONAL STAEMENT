//INPUT 
1234
5678
import java.util.*;
public class condition_if_else{
  public static void main (String[] args){
    int a;
    System.out.println("Enter the password: ");
    Scanner s = new Scanner(System.in);
    a=s.nextInt();
    if (a==1234){
      System.out.println("Your name is: Ayush Kumar Gupta");
    }
    else {
      System.out.println("Please Try Again");
    }
    
  }
}
//OUTPUT 
Enter the password: 
Your name is: Ayush Kumar Gupta
Enter the password: 
Please Try Again
